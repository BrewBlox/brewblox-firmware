  "version": "0.2.0",
  "configurations": [
    {
      "name": "brewblox app gcc",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/target/brewblox-gcc/brewblox",
      "args": [
        "--device_id",
        "123456789012345678901234"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build/target/brewblox-gcc",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing"
          // "ignoreFailures": true
        }
      ],
      "miDebuggerArgs": "-x ~/repos/firmware/tools/pretty.gdbinit",
    },
    {
      "name": "gdb lib test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/lib/test/build/lib_test_runner",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "gdb brewblox app test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/app/brewblox/test/build/brewblox_test_runner",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LSAN_OPTIONS",
          "value": "verbosity=1:log_threads=1"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing"
          // "ignoreFailures": true
        }
      ],
      "miDebuggerArgs": "-x ~/repos/firmware/tools/pretty.gdbinit"
    },
    {
      "name": "gdb controlbox test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/controlbox/build/cbox_test_runner",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing"
          // "ignoreFailures": true
        }
      ],
      "miDebuggerArgs": "-x ~/repos/firmware/tools/pretty.gdbinit"
    },
    {
      "name": "controlbox unit tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/controlbox/build/cbox_test_runner",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "brewblox p1 openocd debug",
      "type": "cortex-debug",
      "request": "attach",
      "servertype": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "./build/target/brewblox-p1/brewblox.elf",
      "debuggerArgs": [
        "-command=./tools/system.gdbinit",
        "-command=./tools/pretty.gdbinit",
      ],
      "rtos": "FreeRTOS",
      "device": "STM32F205RG",
      "configFiles": [
        "/usr/local/share/openocd/scripts/interface/stlink-v2.cfg",
        "/usr/local/share/openocd/scripts/target/stm32f2x.cfg"
      ],
      "armToolchainPath": "/home/elco/source/gcc-arm-none-eabi-5_3-2016q1/bin",
      // sudo apt-get install libpython2.7:i386 for the line below to work
      "gdbpath": "/home/elco/source/gcc-arm-none-eabi-5_3-2016q1/bin/arm-none-eabi-gdb-py", // hidden setting
      "showDevDebugOutput": true,
      "svdFile": "./tools/STM32F215.svd",
    },
    {
      "name": "wifi-test p1 openocd debug",
      "type": "cortex-debug",
      "request": "attach",
      "servertype": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "./build/target/wifi-test-p1/wifi-test.elf",
      "debuggerArgs": [
        "-command=./tools/system.gdbinit",
      ],
      "device": "STM32F205RG",
      "configFiles": [
        "/home/elco/opt/openocd/tcl/interface/stlink.cfg",
        "/home/elco/opt/openocd/tcl/target/stm32f2x.cfg"
      ],
      "armToolchainPath": "/home/elco/source/gcc-arm-none-eabi-_3-2016q1/bin",
      "gdbpath": "/home/elco/source/gcc-arm-none-eabi-_3-2016q1/bin/arm-none-eabi-gdb-py", // hidden setting
      "showDevDebugOutput": true,
      "svdFile": "./tools/STM32F215.svd",
      "rtos": "FreeRTOS",
      "interface": "swd"
    },
    {
      "name": "system p1 openocd debug",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "./platform/spark/device-os/build/target/user-part/platform-8-m/user-part.elf",
      "device": "STM32F205RG",
      "configFiles": [
        "/usr/local/share/openocd/scripts/interface/stlink-v2.cfg",
        "/usr/local/share/openocd/scripts/target/stm32f2x.cfg"
      ],
      "armToolchainPath": "/home/elco/source/gcc-arm-none-eabi-5_3-2016q1/bin",
      "showDevDebugOutput": true,
      "runToMain": true,
      "rtos": "FreeRTOS",
      "interface": "swd"
    }
  ]
}